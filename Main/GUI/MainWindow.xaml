        <Line X1="0" Y1="0" X2="0" Y2="410" Stroke="Black" StrokeThickness="3" Margin="115,10,-105,10"/>
        <ContentControl x:Name="ContentControlWorkspace" HorizontalAlignment="Left" Height="420" Margin="130,0,0,0" VerticalAlignment="Center" Width="660">
        </ContentControl>
    </Grid>
</Window>
